{
  "extends": ["plugin:react-hooks/recommended"],
  "env": {
    "browser": true,
    "es2023": true
  },
  "parserOptions": {
    "ecmaVersion": 2024,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "overrides": [
    {
      "files": ["__tests__/**/*"],
      "env": {
        "jest": true,
        "es2023": true
      }
    }
  ]
}
